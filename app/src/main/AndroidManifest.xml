<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.omunguia.mypuppy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ListadoMascotasFavoritasActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_name_favoritas"
            >
            <!-- soporte para button back para versiones 4.0 o menores-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity">
            </meta-data>
        </activity>


        <activity android:name="com.omunguia.mypuppy.DetalleMascotaActivity"></activity>


    </application>

</manifest>
